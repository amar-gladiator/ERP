<div class="box-inner">
  <div class="box-header well">
    <h2></h2>
  </div>

<div class="box-content">
      <table class="table table-striped">
      <tr>
            <th>Employee Name</th>
            <th>Employee Id</th>
            <th>Employee Deprtment</th>
      </tr>

      <%@employees.each do |emp|%>
          <tr>
                <td col-md-1><%= link_to emp.first_name,employees_profile_path(emp)%></td>
                <td col-md-1><%= emp.id%></td>
                <td col-md-1><%= emp.employee_department.name%></td>
         </tr>
      <%end%>  
      </table>
  </div>
  </div>
  
 

<%= form_for @gradinglevle do |f|%>

	<table >
		<tr>
				<td><%=f.label :name%></td>
				<td><%=f.text_field :name%></td>
		</tr>

		<tr>
				<td><%=f.label :min_score%></td>
				<td><%=f.text_field :min_score%></td>

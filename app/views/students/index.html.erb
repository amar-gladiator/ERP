<div id="content-header">
  <h1>Student Details</h1>
  <h3>View Details</h3>
  <div id="app-back-button">
    <%= link_to image_tag("buttons/back.png",:border => 0), "" %>
  </div>
</div>

<div id="inner-tab-menu">
  <ul>
    <li><%= link_to 'View All','students/view_all'%></li>
    <li><%= link_to 'Advanced','students/advanced_search'%></li>
  </ul>
</div>

<div id="page-yield">

	<div>
		<%= form_for :student, url:{action:'search_ajax'}, method: :get,remote:true do |f|%>
				<%= f.label :Search %>
			   	<%= text_field_tag :search %>
			   	<%= f.submit "Search"%>
			 	<% end %>
 	</div>
<div class="col-md-4"></div> 	
				
<div class="container">
<div class="row"> 
<div class="col-md-2"></div>
	<div class="col-md-8">
		<div id="student_search" style="margin-top:20px">
		</div>	
 	</div>
<div class="col-md-2" class="table table-striped"></div> 	
				
</div>
</div>
	
<div class="col-md-1"></div>
</div>


<div id="box">

  <table border="0" align="center" width="100%" id="table" cellspacing="0"  >
    <tr>
    <td class="col-4"></td>
     <% @batch.class_timings.each do |c|%>
     <td class="td"><%=c.start_time.strftime("%I:%M %p")+"-"+c.end_time.strftime("%I:%M %p")%></td>
      <% end %>
    </tr> 
  
 
    
   <% @batch.weekdays.each do |w|%>
      <tr>
        <td class="td"><div class="name"><%=w.weekday%></div></td>
        <% @batch.class_timings.each do |c|%>
     <td ></td>
      <% end %>
      </tr><!--timetable_tr -->
    <% end %>
  </table><br/><br/><br/>



  <%= form_for :subject,url:{action:'select_subject'},method: :get,remote:true,html:{id:"em"} do |f| %>
<%= f.label :select_subjects %>
<%= f.select :subject_id, @subjects.collect {|b| [b.name,b.id]},{prompt:true},{onchange:"$('#em').submit();"} %>  
<%end%>
</div>

<div id="emp_div"></div>


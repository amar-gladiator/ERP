<%= form_for :subject_select,url:{action: 'generate_subject_report'},method: :get do |f| %>

<b>Select Subject:</b>
     
      <%= f.select :id, @subjects.collect {|s| [s.name,s.id]}, {prompt:true},class:"form-control",style: "width: 300px"%>
     <br>
      <%= f.submit "View", class: "btn btn-default btn-sm" %>
  <% end %>
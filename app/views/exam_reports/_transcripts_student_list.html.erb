<% unless @students.empty?  %>
 <div class="box-inner">
  <div class="box-header well">
    <h2>Students In Batch</h2>
  </div>
  <div class="box-content">
  <ul>
    <% @students.each do |s| %>
      <li>
        <%=link_to s.first_name+" "+s.last_name,exam_reports_student_view_transcripts_path(s),remote:true%>
      </li>
    <% end %>
  </ul>
<% else %>
  <h4 align="center">Students Not Found</h4>
<% end %>
</div>
</div>

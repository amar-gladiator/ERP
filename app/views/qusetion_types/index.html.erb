<h1>Listing qusetion_types</h1>

<table>
  <thead>
    <tr>
      <th>Type</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @qusetion_types.each do |qusetion_type| %>
      <tr>
        <td><%= qusetion_type.type %></td>
        <td><%= link_to 'Show', qusetion_type %></td>
        <td><%= link_to 'Edit', edit_qusetion_type_path(qusetion_type) %></td>
        <td><%= link_to 'Destroy', qusetion_type, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Qusetion type', new_qusetion_type_path %>

<br/>
<div class="row">
  <div class="box col-md-12">
  <div class="box-inner">
    <div class="box-header well">
      <h2>View Time Table</h2>
    </div>
<% if @class_timing.present? %>
    <div class="box-content">
 <table id="datatable" class="table table-striped table-bordered responsive">
    <hr/>
    <tr>
    <td></td>
     <%  @class_timing.each do |c|%>
     <th><%=c.start_time.strftime("%I:%M %p")+"-"+c.end_time.strftime("%I:%M %p")%></th>
      <% end %>
    </tr> 
 
<% @batch.weekdays.reverse_each do |w|%>
      <tr>
        <th><%=w.weekday%></th>
        <% @class_timing.each do |c|%>
        <% @present=TimeTableEntry.find_by_class_timing_id_and_weekday_id_and_time_table_id(c.id,w.id,@time1) %>
        <td>
        <%unless @present.nil? %>
         <font color="red"><%= @present.subject.name %></font><br/>
       <font color="blue"><%= @present.employee.first_name+" "+@present.employee.last_name%></font><br/>
    
  <%else%>
     <%="Not Assigned"%></td>
        <%end%>
     <%end%>
      </tr>
    <% end %>
</table><br/>
<div align="center"><%=link_to 'PDF REPORT',time_tables_time_table_pdf_path(batch_id:@batch,t:@time1,format:"pdf"),class: "btn btn-success"%></div>
 <%end%>
</div>
</div>
</div>
</div>
</div>
</div>


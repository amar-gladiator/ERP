<%unless @timetables.empty? %>
<div>
  <ul class="breadcrumb">
    <li><%= link_to 'Home',home_dashboard_path %></li>
    <li>Employee Time Table</li>
  </ul>
</div><div id="flash"><%= render 'layouts/messages'%></div>
<br/><br/>
<div class="row">
  <div class="col-md-12">
<%= form_for :time,url:{action:'selectTimeEmployee',format:@employee.id},method: :get,remote:true,html:{id: "time_form"} do |f| %>


      <%= f.select :id, @timetables.collect {|t| [t.start_date.strftime("%d %b %Y")+"-"+t.end_date.strftime("%d %b %Y"),t.id]},{prompt:'Select Time'},{onchange:"$('#time_form').submit();",class: "form-control",style:"width:250px"} %>  
    <%end%>
  </div>
</div>
<%else%>
<h4>Time table not assigned to this employee</h4>
<%end%>
<div id="time_div"></div>

<br/><br/>
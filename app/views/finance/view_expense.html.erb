<div>
  <ul class="breadcrumb">
    <li><%= link_to 'Home',home_dashboard_path %></li>
    <li><%= link_to 'Finance',finance_index_path %></li>
    <li><%= link_to 'Transction',finance_transactions_path %></li>
    <li><%= link_to 'Add Expense',finance_new_expense_path %></li>
    <li>View expense</li>
  </ul>
</div>
<div id="flash"><%= render 'layouts/messages'%></div>

<div class="row">
  <div class="col-md-11">
    <div class="pull-right">
      <%= link_to 'Add Expense', 'new_expense', class: "btn btn-primary-link btn-xs" %>
    </div>
  </div>
</div>

<div class="row">
  <div class="box col-md-12">
    <div class="box-inner">
      <div class="box-header well">
        <h2>Select Date </h2>
      </div>

      <div class="box-content">
      <%= form_for :expense, url:{action: 'expense_list'} do |f| %>
    <fieldset>
     
      <div class="row">
        <div class="col-sm-1"></div>
        <div class="col-sm-4">
          <div class="form-group">
            <%= t('Start Date') %>
            <%= f.date_field :start_date,:style =>"width: 200px", class: "form-control"%>
          </div>
          <div class="form-group">
            <%= t('End Date') %>
            <%= f.date_field :end_date,:style =>"width: 200px", class: "form-control"%>
          </div>
        </div>
      </div>

      <div class="col-sm-5 col-sm-offset-5"> 
        <div class="form-actions">               
          <%= f.submit 'View', class: "btn btn-primary"%>
        </div>
      </div>
    </fieldset>        
  <% end %>
</div>
</div>
</div>
</div>



























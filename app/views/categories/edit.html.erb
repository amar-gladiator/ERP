
<div class="continer">
<div class="row">
  <div class="col-md-1"></div>
  <div class="col-md-10">
    <div class="page-header" style="height:10%">
    <div class="row">
      <div class="col-md-6"><h2>Student Category</h2>
      <div><small>Edit</small></div></div>

      <div class="col-md-6"><a href="/setting"><button class="btn btn-success pull-right" style="margin-top:20px">Back</button></a></div>   
    </div>
  
    </div>

  </div>
  <div class="col-md-1"></div>
</div>
</div>

<div class="container" style="margin-top:20px">
	<div class="row">
		<div class="col-md-1"></div>
		<div class="col-md-10">
		<%= form_for @category, method: :patch do |f| %>
 			<div class="col-md-2">
 				<div><%= f.label :edit_category%></div>
 			</div>
   			<div class="col-md-3">
   				<div><%= f.text_field :name ,:style =>"width: 310px",  class: "form-control"%></div>
   			</div>  
   			<div class="col-md-1">
   				
   			</div>
			<div class="col-md-2">
				<div><%= f.submit 'update', class: "btn btn-success"%></div>
			</div>
			<div class="col-md-1">
				<div><%= link_to 'Back', categories_path , class: "btn btn-primary"%></div>
			</div>
  		<% end %>

 		
		</div>
		<div class="col-md-1"></div>
			
		</div>
	</div>
</div>



<link rel="stylesheet" href="/assets/bootstrap-multiselect.css" type="text/css" />
<script type="text/javascript" src="http://code.jquery.com/jquery-1.8.2.js"></script>

<script type="text/javascript" src="/assets/bootstrap-multiselect.js"></script>


<div class="row">
  <div class="box col-md-9">
    <div class="box-inner">
      <div class="box-content">
        <fieldset>
          <%=select_tag("batches[]",options_for_select(@batches.collect { |ff| [ff.name, ff.id] },@batches.collect { |fs| [fs.id] }),{:multiple=>true, :id => "batches"})%>
          
        </fieldset>
      </div>
    </div>
  </div>
</div>


<script type="text/javascript">
$(function() {
$('#batches').multiselect({
includeSelectAllOption: true
});

});
</script>
                   
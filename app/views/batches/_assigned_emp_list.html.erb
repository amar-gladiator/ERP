
<h2>Assigned Employee</h2>

<table class="tabel" style="width:400px">
	<%  @assign_employees.each do |emp|%>
	<% @employee = Employee.find(emp) %>
	<tr>
		<td><%=@employee.first_name%></td>
		<td><%= link_to "Delete",batches_remove_employee_path(@batch,@employee),method: :delete,data: { confirm: 'Are you sure?' } ,remote:true%></td>
	</tr>
	<%end%>
</table>


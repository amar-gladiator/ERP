<h2 align="center">Initial Batch</h2>
<%= form_for ([@course,@course.batches.build ]) do |b|%>
<table class="table">

<tbody>
<tr><td><%=b.label :name%></td>
<td><%=b.text_field :name ,:style =>"width: 310px", :placeholder=>"batch" , class: "form-control"%></td>
</tr>
<tr>
<td><%=b.label :start_date%></td>
<td><%=b.text_field :start_date,:style =>"width: 310px", :placeholder=>"date" , class: "form-control"%></td>
</tr>
<tr>
<td><%=b.label :end_date%></td>
<td><%= b.text_field :end_date ,:style =>"width: 310px", :placeholder=>"date" , class: "form-control"%></td>
</tr>
<tr>
<td colspan="2" ><%=b.submit 'Create',class: "btn btn-primary"%></td></tr>

<%end%>
<%end%>
</tbody>
</table>